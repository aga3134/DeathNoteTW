		<script src="https://cdn.jsdelivr.net/npm/vue"></script>

		<div id="app" class="death-note center">
			<div class="cover-top">
				<div id="coverContent">
					<div id="svgMovie"></div>
					<div class="title">
						台灣の<br>死亡筆記本
					</div>
					
					<div class="chapter">
